<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/">
    <rdfs:label xml:lang="en-US">Attribution</rdfs:label>
    <rdfs:comment xml:lang="en-US">GEDCOM X Attribution Vocabulary</rdfs:comment>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/AttributableResource">
    <rdfs:label xml:lang="en-US">AttributableResource</rdfs:label>
    <rdfs:comment xml:lang="en-US">A resource to which attribution can be assigned.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/AttributionResource">
    <rdfs:label xml:lang="en-US">AttributionResource</rdfs:label>
    <rdfs:comment xml:lang="en-US">An attribution.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/attribution">
    <rdfs:label xml:lang="en-US">Attribution</rdfs:label>
    <rdfs:comment xml:lang="en-US">The attribution of a resource.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://gedcomx.org/attribution/v1/AttributionResource"/>
    <rdfs:domain rdf:resource="http://gedcomx.org/attribution/v1/AttributableResource"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/contributor">
    <rdfs:label xml:lang="en-US">Contributor</rdfs:label>
    <rdfs:comment xml:lang="en-US">The contributor to which something is attributed.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/contributor"/>
    <rdfs:domain rdf:resource="http://gedcomx.org/attribution/v1/AttributionResource"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/timestamp">
    <rdfs:label xml:lang="en-US">Timestamp</rdfs:label>
    <rdfs:comment xml:lang="en-US">The timestamp of an attribution.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/dateSubmitted"/>
    <rdfs:domain rdf:resource="http://gedcomx.org/attribution/v1/AttributionResource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://gedcomx.org/attribution/v1/explanation">
    <rdfs:label xml:lang="en-US">Explanation</rdfs:label>
    <rdfs:comment xml:lang="en-US">An explanation for the resource being attributed.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://gedcomx.org/attribution/v1/"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://gedcomx.org/attribution/v1/AttributionResource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Description>

</rdf:RDF>